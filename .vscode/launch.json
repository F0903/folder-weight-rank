    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'fwr'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=fwr",
                    "--package=folder-weight-rank"
                ],
                "filter": {
                    "name": "fwr",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "temp test",
            "cargo": {
                "args": [
                    "build",
                    "--bin=fwr",
                    "--package=folder-weight-rank"
                ],
                "filter": {
                    "name": "fwr",
                    "kind": "bin"
                }
            },
            "args": ["-t", "E:/"],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'fwr' search E drive with timing",
            "cargo": {
                "args": [
                    "build",
                    "--bin=fwr",
                    "--package=folder-weight-rank"
                ],
                "filter": {
                    "name": "fwr",
                    "kind": "bin"
                }
            },
            "args": ["-t", "E:/"],
